{
  "name": "baseline-ci-action",
  "version": "1.0.0",
  "description": "GitHub Action that analyzes PRs for web feature compatibility using Baseline data",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && ncc build lib/index.js -o dist --source-map --license licenses.txt",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "github-action",
    "baseline",
    "web-features",
    "compatibility"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.9.0",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "web-features": "^3.3.0"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^24.6.2",
    "@vercel/ncc": "^0.38.4",
    "@vitest/ui": "^3.2.4",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
